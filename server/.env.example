# Example environment variables for the server
# Copy this file to `.env` and fill in real secret values locally.

# Google Gemini API key (generative AI)
GEMINI_API_KEY=

# OpenAI API key (fallback)
OPENAI_API_KEY=

# Optional: override OpenAI model (e.g. gpt-4o, gpt-4, gpt-3.5-turbo)
OPENAI_MODEL=

# DeepSeek/other AI provider key (if used)
DEEPSEEKAI_API_KEY=

# Hugging Face API key
HUGGINGFACE_API_KEY=

# MongoDB connection string
MONGO_URI=

# YouTube API key (used by youtubeService)
YOUTUBE_API_KEY=

# Auth0 or other auth provider
AUTH0_AUDIENCE=
AUTH0_ISSUER=

# Server/runtime settings
PORT=5000
NODE_ENV=development
